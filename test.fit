// Sample FitScript program

routine "Leg Day" {
    let energy = read_sensor(ENERGY_LEVEL);

    exercise "Squat" {
        sets: 3;
        reps: 8;
        weight: 110kg;
    }

    if (energy > 7) {
        exercise "Leg Press" {
            sets: 3;
            reps: 12;
            weight: 225kg;
        }
    }

    loop 3 times {
        exercise "Calf Raises" {
            sets: 1;
            reps: 15;
        }
    }
}
